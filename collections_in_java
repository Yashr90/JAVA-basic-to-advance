package app;
import java.util.List;
import java.util.ArrayList;
public class Collecetions {
public static void main(String[] args) {
	List<Integer> numbers = new ArrayList<>();
	System.out.println("My number list Before:"+numbers);
	numbers.add(100);
	numbers.add(200);
	numbers.add(300);
	numbers.add(400);
	numbers.add(500);
	System.out.println("My number list After:"+numbers);
	numbers.remove(3);
	System.out.println("My number :"+numbers);
	for(int nums :numbers) {
		System.out.println("My number with index:"+nums);
	}
}
}
